# Declare files that will always have LF line endings on checkout.
*.py text eol=lf
*.sh text eol=lf
*.md text eol=lf
*.mdc text eol=lf
*.txt text eol=lf
*.yaml text eol=lf
*.yml text eol=lf
*.ini text eol=lf
*.conf text eol=lf
*.log text eol=lf
*.csv text eol=lf
*.json text eol=lf
*.cmd text eol=crlf
*.bat text eol=crlf
*.ps1 text eol=crlf
*.psm1 text eol=crlf
*.psd1 text eol=crlf
*.ps1xml text eol=crlf
.gitignore eol=lf
.dockerignore eol=lf
Dockerfile eol=crlf